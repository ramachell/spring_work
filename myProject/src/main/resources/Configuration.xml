<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- 자주 사용하는 type 의 별칭을 지정가능 -->
	<typeAliases>

		<typeAlias type="com.gura.spsp.gs25.dto.Gs25Dto"
			alias="gs25dto" />
		<typeAlias type="com.gura.spsp.users.dto.UsersDto" />
	</typeAliases>

	<!-- sql 문을 작성한 Mapper xml 문서가 어디에 있는지 목록을 작성해야 한다. -->
	<mappers>
		<mapper resource="mapper/UsersMapper.xml" />

		<mapper resource="mapper/Gs25Mapper.xml" />
	</mappers>
</configuration>